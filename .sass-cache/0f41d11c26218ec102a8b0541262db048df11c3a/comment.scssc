.post{
	.figure_first{
		margin:0 0 -87px;
	}
}
.mess-list{
	background:#f8f8f8;
	.mess{
		padding:0;
		border-top:1px solid #e6e6e6;
		background:none;
		box-shadow:none;
		@include transition(background, .2s, ease-in-out);
		&:hover{
			background:#fbfbfb;
			.button_reply{opacity:1;}
		}
	}
	.mess__author{background:#fbf9e5;}
}
.post__socials + .mess-list > *:first-child{
	box-shadow: 0 10px 8px -8px rgba(0, 0, 0, 0.05) inset;
}
.mess{
	box-shadow: 0 10px 8px -8px rgba(0, 0, 0, 0.05) inset;
	.mess__text{
		margin: auto;
		width: 640px;
		padding-bottom:1px;
		font-size: 16px;
		font:16px/1.8 Cambria,georgia,serif;
		p{
			font:16px/1.8 Cambria,georgia,serif;
			margin-bottom:1.6em;
		}
		h1{
			font:28px/1.25 Cambria,georgia,serif;
			color:#222;
		}
		ol,ul{
			font-size: 16px;
			margin-left: 2em;
			margin-top: -10px;
			margin-bottom: 10px;
		}
		.mess__details{
			font-size:14px;
			line-height:1.15;
			margin:0;
			font-family: Helvetica,arial,sans-serif;
		}
	}
	figure{
		margin-bottom:30px;
		img{border-radius:3px;}
	}
	.mess__text-holder{
		padding-top:36px;
		margin-bottom:35px;
	}
	.mess__userpic{
		border-radius:50%;
		width: 50px;
		height: 50px;
		border: 2px solid #fff;
		box-shadow:0 1px 1px rgba(black,0.2);
		float: left;
		margin-right:8px;
	}
	.mess__username{
		font-size: 11px;
		color:#9f9999;
		font-size:11px;
		line-height:1.3;
		float: left;
		margin:8px 0 0 0;
		width:60px;
		a{
		   color: #9f9999;
		   text-decoration: none;
		}
	}
	.mess__meta{
		position:absolute;
		top:20px;
		right:20px;
		height:22px;
		width:150px;
		font-family:Arial,sans-serif;
	}
	.mess__rating{
		border:1px solid #dfdfdf;
		color: #9c9c9c;
		font-weight:bold;
		font-size:14px;
		background:#fff;
		@include background-image(linear-gradient(top, rgba(255,255,255,0.05) 0, rgba(0,0,0,.05) 100%));
		height:20px;
		line-height: 20px;
		padding:0 3px;
		float:right;
		border-radius:10px;
		margin-left:12px;
		div{
			cursor:pointer;
			height:20px;
		}
	}
	.mess__rating-num{
		float:left;
		padding:0 5px;
	}
	.mess__rating-p{
		@include sized-sprite($icons, "arr-t");
		float:left;
		border-right:1px solid #dfdfdf;
	}
	.mess__rating-m{
		@include sized-sprite($icons, "arr-b");
		float:left;
		border-right:1px solid #dfdfdf;
	}
	.mess__remove, .mess__edit{
		background:#fff;
		@include background-image(linear-gradient(top, rgba(255,255,255,0.05) 0, rgba(0,0,0,.05) 100%));
		height:20px;
		width:20px;
		border-radius:10px;
		border:1px solid #dfdfdf;
		cursor:pointer;
		float:right;
		line-height:18px;
		text-align:center;
		vertical-align:middle;
		margin-left:12px;
		div{
			overflow:hidden;
			text-indent:-9999px;
			display:inline-block;
			vertical-align:middle;
		}
	}
	.mess__remove div{@include sized-sprite($icons, "remove");}
	.mess__edit div{@include sized-sprite($icons, "edit");}
	.mess__time{
		color:#9f9999;
		position:absolute;
		font-size:11px;
		bottom:21px;
		right:23px;
	}
	.mess__user:hover{
		.user-more-arr{display:block;}
	}
}

.button_reply{
	position:absolute;
	bottom:19px;
	left:15px;
	opacity:0;
	@include transition(all, .2s, ease-in-out);
}
.user-more-arr{
	@include sized-sprite($icons, "arr-b-2");
	position:absolute;
	bottom:5px;
	left:53px;
	display:none;
	cursor:pointer;
	@include transition(all, .1s, ease-in-out);
}

.mess_reply{
	.mess__text{
		background:#fff;
		border:1px solid #e6e6e6;
		border-width:0 1px;
		background:#fff url(../img/bg-comment.png) repeat-y 100% 0;
	}
	.mess__text-holder{
		padding:35px 50px 0;
		form{margin: 5px 0 -15px -10px;}
	}
	figure{
		margin:0 -50px 30px;
		text-align:center;
	}
	form{width:550px;}
	.mess__visual textarea,.mess__visual .textarea{
		width:516px;
		height:127px;
		resize: none;
	}
}
.mess_l1 .mess__text, .mess__text-ans{
	border-radius:4px 4px 0 0;
	margin-top:-5px;
	border-top:1px solid #e6e6e6;
	position:relative;
	&:before{
		@include sized-sprite($icons, "comment-top");
		content:"";
		position:absolute;
		top:-13px;
		left:49px;
	}
}
.mess__text-ans{
	background:#fff;
	border:1px solid #e6e6e6;
	background:#fff url(../img/bg-comment.png) repeat-y 100% 0;
	border-radius:4px;
	padding:35px 50px 15px;
	margin-top:15px;
	box-shadow:0 1px 3px 0 rgba(0,0,0,0.05);
}
.mess__text-ans-l2{
	padding:35px 50px 15px 100px;
	border-top:1px solid #e6e6e6;
	background:url(../img/bg-comment.png) repeat-y -200px 0;
	margin:0 -50px -15px;
	form{
		width:500px;
		margin-left:-5px;
	}
	textarea,.textarea{
		height:127px;
	}
	.mess__visual textarea,.mess__visual .textarea{width:466px;}
}
.mess_l2{
	.mess__text{background-position:-200px 0;}
	.mess__text-holder{padding-left:100px;}
	form{width:500px;}
	.mess__visual textarea,.mess__visual .textarea{width:466px}
}
.mess_l3{
	.mess__text{	background-position:-150px 0;}
	.mess__text-holder{
		padding-left:150px;
		form{width:450px;}
		.mess__visual textarea,.mess__visual .textarea{width:416px}
	}
}
.mess_l4{
	.mess__text{
		background-position:-100px 0;
	}
	.mess__text-holder{
		padding-left:200px;
		form{width:400px;}
		.mess__visual textarea,.mess__visual .textarea{width:366px}
	}
}

.mess_l5{
	.mess__text{
		background-position:-50px 0;
	}
	.mess__text-holder{
		padding-left:250px;
		form{width:350px;}
		.mess__visual textarea,.mess__visual .textarea{width:316px}
	}
}
.mess_last{
	position:relative;

	.mess__text{
		border-radius:0 0 4px 4px;
		margin-bottom:-5px;
		border-bottom:1px solid #e6e6e6;
		position:relative;
		z-index:10;
	}
	&:hover:after{
		display: block;
	}
	&:after{
		position: absolute;
		display: none;
		width:1000px;
		height:6px;
		left:0;
		bottom:0;
		background:#f8f8f8;
		content:"";
		@include transition(all, .2s, ease-in-out);
	}
	&:last-child .mess__text{box-shadow: 0 10px 8px -8px rgba(0, 0, 0, 0.05);}
}
.mess__like{
	@include sized-sprite($icons, "like1");
	cursor:pointer;
	float:right;
}
.mess__share{
	@include sized-sprite($icons, "share1");
	cursor:pointer;
	float:right;
}
.mess__comment{
	@include sized-sprite($icons, "commenticon1");
	cursor:pointer;
	float:right;
}
.mess__userlist{
	display:inline-block;
	padding:17px 0 8px;
	.mess__userpic{
		margin-right:8px;
		height:46px;
		width:46px;
	}
}
.mess_add, .mess-list .mess_add{padding: 50px 0 36px;}
.mess_add-closed, .mess-list .mess_add-closed{
	padding:20px 0 6px;
	.mess__user{display: none;}
	.textformat{display: none;}
	.addphoto{display: none;}
	.button{display: none;}
	textarea,.textarea{padding: 12px 16px;}
}

.user-inf{
	background:#fff;
	.user-inf__avatar{
		border-radius:50%;
		border:2px solid #ebebeb;
	}
	h2{
		font-weight:bold;
		font-size:18px;
		line-height:1.4;
		a{
			color:#000;
			text-decoration:none;
		}
	}
	p{
		font:14px/1.2 Cambria,georgia,serif;
		color:#1d1d1d;
		opacity:0.6;
	}
	.button{padding:2px 10px;}
	.stats{
		border-top:1px solid #ebe7e7;
		overflow:hidden;
		list-style:none;
		width:230px;
		line-height:1.4;
		li{
			float:left;
			border-left:1px solid #ebe7e7;
			font-size:11px;
			color:#999;
			text-align:left;
			a{
				color:#b3b3b3;
				text-decoration:none;
				display:block;
				height:41px;
				padding:5px 0 0 15px;
				width:55px;
			}
			&:first-child{border:0;}
			strong{
				display: block;
				color:#1d1d1d;
				font-size:14px;
				margin-top:4px;
			}
		}
	}
}

.user-inf_popup{
	width:200px;
	display:none;
	text-align:center;
	position:absolute;
	top:59px;
	left:13px;
	padding:0 15px;
	border:1px solid #ebe7e7;
	border-radius:5px;
	z-index:250;
	box-shadow:0 1px 2px 0 rgba(0,0,0,0.1);
	&:before{
		@include sized-sprite($icons, "user-top");
		content:"";
		position:absolute;
		top:-10px;
		left:34px;
	}
	.user-inf__avatar{
		height:100px;
		width:100px;
		margin:-12px auto 5px;
		display:block;
	}
	h2{margin-bottom:4px;}
	p{margin-bottom:12px;}
	.button{margin-bottom:9px;}
	.button{padding:2px 12px;}
	.stats{
		margin:0 -15px;
		width:225px;
		padding-left:5px;
	}
}
.mess .button_reply, .button_reply{
	padding: 2px 10px 2px 8px;
	span{
		@include sized-sprite($icons, "arr-t-2");
		margin-right: 6px;
		display:inline-block;
		vertical-align:middle;
		position: relative;
		top:-1px;
	}
}

.post__load-all{
	border-top: 1px solid #e5e5e5;
	height: 48px;
	margin-top: 12px;
	overflow: hidden;
	position: relative;
	line-height:43px;
	padding:0 19px;
	background:#fff;
	a{
		font-size:14px;
		color:#afafaf;
		text-decoration:none;
		font-weight:bold;
		margin-left:134px;
	}
	.loader{
		margin:0 3px 0 2px;
		vertical-align:middle;
	}
}


.hr{
	margin:14px 0;
	border-top:1px solid #ddd;
	border-bottom:1px solid #f1f1f1;
}


.post-list{
	padding-top:43px;
	.mess-list .mess:first-child{
		box-shadow: 0 10px 8px -8px rgba(0, 0, 0, 0.05) inset;
	}
}
.post_item{
	background: #fff;
	box-shadow: 0 2px 0 rgba(0,0,0,0.05), 0 0 1px rgba(0,0,0,0.2);
	margin: 0 auto;
	padding: 0;
	position: relative;
	width: 1000px;
	z-index: 9;
	figure img{vertical-align:top;}
}

.post__header{
	display:inline-block;
	width:960px;
	padding:20px 20px 16px;
	.post__retweet-author, .post__author-pic{
		border-radius:50%;
		height:60px;
		width:60px;
		border:2px solid #d9d9d9;
		float:left;
		margin:0 11px 0 0;
	}
	.post__author-pic{margin-right:16px;}
	.post__shareicon{margin-top:20px;}
	.post__author{
		float:left;
		position: relative;
		font-weight:bold;
		.user-more-arr{
			left:59px;
			bottom:1px;
		}
		&:hover{
			.user-more-arr{display: block;}
			.post__author-name{color:#a3a3a3;}
		}
	}
	.post__author-name{
		color:#000;
		font-size:18px;
		vertical-align:middle;
		float:left;
		text-decoration: none;
		margin-top:19px;
		@include transition(all, .1s, ease-in-out);
	}
	.user-inf_popup{
		left:19px;
		top:73px;
	}
}

.post__meta{
	float:right;
	margin-top:-5px;
	span{
		color: #b8b8b8;
		font-size:11px;
	}
}

.control-nav-holder{
	display: inline-block;
	position: relative;
	margin-left:3px;
	&:hover{
		.control-nav__arr{@include sized-sprite($icons, "arr-btn-active");}
	}
}
.control-nav__arr{
	@include sized-sprite($icons, "arr-btn");
	display: inline-block;
	vertical-align:middle;
	cursor:pointer;
}
.control-nav{
	position: absolute;
	width:143px;
	background:#fff;
	list-style-type: none;
	border:1px solid #ebe7e7;
	border-radius:5px;
	z-index:15;
	box-shadow:0 1px 2px 0 rgba(0,0,0,0.1);
	top:33px;
	right:-15px;
	padding:8px 0 7px;
	display: none;
	&:before{
		@include sized-sprite($icons, "user-top");
		content:"";
		position:absolute;
		top:-10px;
		right:12px;
	}
	a{
		color:#a8a8a8;
		width:113px;
		padding:4px 15px 5px;
		font-size:12px;
		text-decoration: none;
		display: block;
		font-weight:bold;
		position: relative;
		&:hover{
			color:#11b0eb;
			background:#f3f3f3;
		}
		span{
			color:#11b0eb;
			position: absolute;
			top:3px;
			right:14px;
		}
	}
	.sign-out{
		border-top:1px solid #e5e5e5;
		margin:3px 0 -4px;
		padding-top:3px;
	}
}

.post-list__manag{
	text-align: center;
	.button{
		padding:5px 26px 6px;
	}
}
.post-list__year{
	color:#9c9c9c;
	font:bold 18px Arial, sans-serif;
	text-shadow:0 1px 0 #fff;
	text-decoration: none;
	line-height:1.3;
	&:hover{color:#c0c0c0;}
}
.post-list__month{
	line-height: 1.3;
	padding: 4px 0 0;
	li{
		display: inline-block;
		margin: 0 3px;
	}
	a{
		color:#9c9c9c;
		font:bold 14px Arial, sans-serif;
		text-shadow:0 1px 0 #fff;
		text-decoration: none;
	}
	.active a{color:#11b0eb;}
	.disabled a{
		color:#c0c0c0;
		cursor:default;
	}
}
.read-more{
	@include sized-sprite($icons, "read-more");
	font: 0/0 a;
	text-shadow:none;
	color:transparent;
	display:inline-block;
	vertical-align:middle;
}
.post-mini{
	overflow: hidden;
	color:#9c9c9c;
	font:14px Arial, sans-serif;
	width:960px;
	margin:14px auto;
	font-weight:bold;
	display: none;
	.post-mini__expand{
		float:right;
		font-size:11px;
		font-weight:normal;
		border:1px solid #d3d3d3;
		height:17px;
		line-height:17px;
		border-radius:8px;
		padding:0 9px;
		cursor:pointer;
	}
